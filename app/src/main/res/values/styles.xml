<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="AppTheme.Translucent.NoTitleBar" parent="Theme.AppCompat.NoActionBar">
        <item name="android:windowBackground">@color/realTranslucent</item>
        <item name="android:windowIsTranslucent">true</item>
    </style>

    <style name="AppTheme.Default" parent="Theme.AppCompat">
        <item name="android:actionOverflowButtonStyle">@style/OverflowStyle</item>
    </style>

    <style name="AppTheme.Default.Light" parent="Theme.AppCompat.Light">
        <item name="android:actionOverflowButtonStyle">@style/OverflowStyle</item>
    </style>

    <style name="AppTheme.Default.Light.White" parent="Theme.AppCompat.Light">
        <item name="android:actionOverflowButtonStyle">@style/OverflowStyle.LightBackground</item>
    </style>

    <style name="AppTheme.Default.Blue" parent="AppTheme.Default.Light">
        <item name="colorPrimaryDark">#026FDE</item>
        <item name="colorPrimary">#0383E9</item>
    </style>

    <style name="AppTheme.Default.Yellow" parent="AppTheme.Default.Light">
        <item name="colorPrimaryDark">#f7a500</item>
        <item name="colorPrimary">#ffbf00</item>
    </style>

    <style name="AppTheme.Default.Orange" parent="AppTheme.Default.Light">
        <item name="colorPrimaryDark">#c43e00</item>
        <item name="colorPrimary">#ff6f00</item>
    </style>

    <style name="AppTheme.Default.Green" parent="AppTheme.Default.Light">
        <item name="colorPrimaryDark">#009624</item>
        <item name="colorPrimary">#00c853</item>
    </style>

    <style name="AppTheme.Default.Pink" parent="AppTheme.Default.Light">
        <item name="colorPrimaryDark">#ec407a</item>
        <item name="colorPrimary">#FF4081</item>
    </style>

    <style name="AppTheme.Default.Red" parent="AppTheme.Default.Light">
        <item name="colorPrimaryDark">#9b0000</item>
        <item name="colorPrimary">#d50000</item>
    </style>

    <style name="AppTheme.Default.White" parent="AppTheme.Default.Light.White">
        <item name="colorPrimaryDark">#eeeeee</item>
        <item name="colorPrimary">#fafafa</item>
    </style>

    <style name="AppTheme.Default.Dialog" parent="AppTheme.Default">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowBackground">@color/realTranslucent</item>
        <item name="android:windowIsTranslucent">true</item>
    </style>

    <style name="AppTheme.Default.Dialog.Blue" parent="AppTheme.Default.Blue">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowBackground">@color/realTranslucent</item>
        <item name="android:windowIsTranslucent">true</item>
    </style>

    <style name="AppTheme.Default.Dialog.Yellow" parent="AppTheme.Default.Yellow">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowBackground">@color/realTranslucent</item>
        <item name="android:windowIsTranslucent">true</item>
    </style>

    <style name="AppTheme.Default.Dialog.Orange" parent="AppTheme.Default.Orange">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowBackground">@color/realTranslucent</item>
        <item name="android:windowIsTranslucent">true</item>
    </style>

    <style name="AppTheme.Default.Dialog.Green" parent="AppTheme.Default.Green">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowBackground">@color/realTranslucent</item>
        <item name="android:windowIsTranslucent">true</item>
    </style>

    <style name="AppTheme.Default.Dialog.Pink" parent="AppTheme.Default.Pink">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowBackground">@color/realTranslucent</item>
        <item name="android:windowIsTranslucent">true</item>
    </style>

    <style name="AppTheme.Default.Dialog.Red" parent="AppTheme.Default.Red">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowBackground">@color/realTranslucent</item>
        <item name="android:windowIsTranslucent">true</item>
    </style>

    <style name="AppTheme.Default.Dialog.White" parent="AppTheme.Default.White">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowBackground">@color/realTranslucent</item>
        <item name="android:windowIsTranslucent">true</item>
    </style>

    <style name="OverflowStyle" parent="Widget.AppCompat.ActionButton.Overflow">
        <item name="android:src">@drawable/ic_action_more</item>
    </style>

    <style name="OverflowStyle.LightBackground" parent="Widget.AppCompat.ActionButton.Overflow">
        <item name="android:src">@drawable/ic_action_more_light</item>
    </style>
</resources>
